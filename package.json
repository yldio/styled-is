{
  "name": "styled-is",
  "version": "1.2.0",
  "license": "MPL-2.0",
  "description": "Flag utility for styled-components",
  "keywords": [
    "flag",
    "flags",
    "react",
    "css",
    "css-in-js",
    "styled-components"
  ],
  "repository": "github:yldio/styled-is",
  "main": "dist/styled-is.umd.js",
  "jsnext:main": "dist/styled-is.es.js",
  "module": "dist/styled-is.es.js",
  "entry": "src/index.js",
  "directories": {
    "lib": "dist"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "fmt": "prettier --write --single-quote src/*.js",
    "test": "NODE_ENV=test nyc --reporter=lcov --reporter=text ava",
    "build": "bup",
    "prepublish": "bup"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bup": "^4.1.6",
    "eslint": "^5.9.0",
    "eslint-config-joyent-portal": "^3.3.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-config-xo-space": "^0.20.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "nyc": "^13.1.0",
    "prettier": "^1.15.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.1"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "ava": {
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "peerDependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.1"
  }
}
